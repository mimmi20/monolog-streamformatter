{
  "name": "mimmi20/monolog-streamformatter",
  "description": "Provides a Stream Formatter for Monolog",
  "license": "MIT",
  "type": "library",
  "authors": [
    {
      "name": "Thomas MÃ¼ller",
      "homepage": "https://github.com/mimmi20",
      "role": "Developer"
    }
  ],
  "homepage": "https://github.com/mimmi20/monolog-streamformatter",
  "support": {
    "issues": "https://github.com/mimmi20/monolog-streamformatter/issues",
    "source": "https://github.com/mimmi20/monolog-streamformatter"
  },
  "require": {
    "php": "^8.1.0",
    "ext-mbstring": "*",
    "monolog/monolog": "^3.5.0",
    "psr/container": "^1.1.2 || ^2.0.2",
    "symfony/console": "^v6.4.4"
  },
  "require-dev": {
    "ext-ctype": "*",
    "ext-dom": "*",
    "ext-json": "*",
    "ext-simplexml": "*",
    "ext-tokenizer": "*",
    "ext-xml": "*",
    "ext-xmlwriter": "*",
    "infection/infection": "^0.27.11",
    "laminas/laminas-modulemanager": "^2.15.0",
    "laminas/laminas-servicemanager": "^3.22.1 || ^4.0.0",
    "mimmi20/coding-standard": "^5.2.19",
    "nikic/php-parser": "^v4.19.1",
    "phpstan/extension-installer": "^1.3.1",
    "phpstan/phpstan": "^1.10.66",
    "phpstan/phpstan-deprecation-rules": "^1.1.4",
    "phpstan/phpstan-phpunit": "^1.3.16",
    "phpunit/phpunit": "^10.5.16",
    "rector/rector": "^1.0.3"
  },
  "autoload": {
    "psr-4": {
      "Mimmi20\\Monolog\\Formatter\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Mimmi20Test\\Monolog\\Formatter\\": "tests/"
    }
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true,
      "phpstan/extension-installer": true,
      "infection/extension-installer": true
    },
    "lock": false
  },
  "extra": {
    "laminas": {
      "component": "Mimmi20\\Monolog\\Formatter",
      "config-provider": "Mimmi20\\Monolog\\Formatter\\ConfigProvider"
    }
  }
}
