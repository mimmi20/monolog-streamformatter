{
  "name": "mimmi20/monolog-streamformatter",
  "description": "Provides a Stream Formatter for Monolog",
  "license": "MIT",
  "type": "library",
  "authors": [
    {
      "name": "Thomas MÃ¼ller",
      "homepage": "https://github.com/mimmi20",
      "role": "Developer"
    }
  ],
  "homepage": "https://github.com/mimmi20/monolog-streamformatter",
  "support": {
    "issues": "https://github.com/mimmi20/monolog-streamformatter/issues",
    "source": "https://github.com/mimmi20/monolog-streamformatter"
  },
  "require": {
    "php": "^8.1.0",
    "ext-mbstring": "*",
    "monolog/monolog": "^3.2.0",
    "symfony/console": "^v6.2.5"
  },
  "require-dev": {
    "ext-dom": "*",
    "ext-json": "*",
    "ext-simplexml": "*",
    "ext-tokenizer": "*",
    "ext-xml": "*",
    "ext-xmlwriter": "*",
    "infection/infection": "^0.26.18",
    "mimmi20/coding-standard": "^3.0.26",
    "nikic/php-parser": "^v4.15.3",
    "phpstan/extension-installer": "^1.2.0",
    "phpstan/phpstan": "^1.9.14",
    "phpstan/phpstan-deprecation-rules": "^1.1.1",
    "phpstan/phpstan-phpunit": "^1.3.3",
    "phpunit/phpunit": "^9.5.28"
  },
  "autoload": {
    "psr-4": {
      "Mimmi20\\Monolog\\Formatter\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Mimmi20Test\\Monolog\\Formatter\\": "tests/"
    }
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true,
      "phpstan/extension-installer": true,
      "infection/extension-installer": true
    },
    "lock": false
  }
}
