{
  "name": "mimmi20/monolog-streamformatter",
  "description": "Provides a Stream Formatter for Monolog",
  "license": "MIT",
  "type": "library",
  "authors": [
    {
      "name": "Thomas MÃ¼ller",
      "homepage": "https://github.com/mimmi20",
      "role": "Developer"
    }
  ],
  "homepage": "https://github.com/mimmi20/monolog-streamformatter",
  "support": {
    "issues": "https://github.com/mimmi20/monolog-streamformatter/issues",
    "source": "https://github.com/mimmi20/monolog-streamformatter"
  },
  "require": {
    "php": "~8.3.0 || ~8.4.0 || 8.5.0",
    "monolog/monolog": "^3.9.0",
    "psr/container": "^1.1.2 || ^2.0.2",
    "symfony/console": "^7.3.0"
  },
  "require-dev": {
    "ext-ctype": "*",
    "ext-dom": "*",
    "ext-json": "*",
    "ext-simplexml": "*",
    "ext-tokenizer": "*",
    "ext-xml": "*",
    "ext-xmlwriter": "*",
    "infection/infection": "^0.29.14",
    "laminas/laminas-modulemanager": "^2.17.0",
    "laminas/laminas-servicemanager": "^4.4.0",
    "mimmi20/coding-standard": "^6.0.33",
    "nikic/php-parser": "^5.5.0",
    "phpstan/extension-installer": "^1.4.3",
    "phpstan/phpstan": "^2.1.17",
    "phpstan/phpstan-deprecation-rules": "^2.0.3",
    "phpunit/phpunit": "^12.2.1",
    "rector/rector": "^2.0.18",
    "rector/type-perfect": "^2.1.0",
    "shipmonk/composer-dependency-analyser": "^1.8.3",
    "symfony/process": "^7.3.0",
    "symplify/phpstan-rules": "^14.6.9",
    "tomasvotruba/cognitive-complexity": "^1.0.0",
    "tomasvotruba/type-coverage": "^2.0.2",
    "tomasvotruba/unused-public": "^2.0.1"
  },
  "autoload": {
    "psr-4": {
      "Mimmi20\\Monolog\\Formatter\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Mimmi20Test\\Monolog\\Formatter\\": "tests/"
    }
  },
  "config": {
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true,
      "infection/extension-installer": true,
      "phpstan/extension-installer": true
    },
    "lock": false,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "extra": {
    "laminas": {
      "component": "Mimmi20\\Monolog\\Formatter",
      "config-provider": "Mimmi20\\Monolog\\Formatter\\ConfigProvider"
    }
  }
}
